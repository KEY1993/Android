<resources xmlns:android="http://schemas.android.com/apk/res/android">
<style name="customspinner" parent="android:style/Widget.Spinner">

<item name="android:background">@drawable/expander</item>
</style>

	<style name="NoticeRatingBar" parent="@android:style/Widget.RatingBar">
		<item name="android:progressDrawable">@drawable/noticead_ratingbar</item>
		<item name="android:minHeight">14dip</item>
		<item name="android:maxHeight">14dip</item>
	</style>
	<style name="noticead_data_text">
	    <item name="android:textSize">13sp</item>
	    <item name="android:textColor">#000000</item>
	    <item name="android:shadowRadius">2</item>
	    <item name="android:shadowColor">#6f7989</item>
	</style>
	<style name="CustomDialogTheme" parent="@android:style/Theme.Dialog">
	    <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
	</style>
</resources>
